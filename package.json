{
  "name": "shopee-scraper-system",
  "version": "1.0.0",
  "description": "Sistem scraper Shopee profesional dengan arsitektur modular",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "migrate": "node src/scripts/migrate.js",
    "seed": "node src/scripts/seed.js",
    "worker": "node src/workers/index.js",
    "scheduler": "node src/scheduler/index.js",
    "queue:start": "node src/scripts/start-queue-system.js start",
    "queue:manager": "node src/scripts/start-queue-system.js queue",
    "queue:workers": "node src/scripts/start-queue-system.js workers",
    "queue:dashboard": "node src/scripts/start-queue-system.js dashboard",
    "queue:scheduler": "node src/scripts/start-queue-system.js scheduler",
    "queue:demo": "node examples/queue-system-usage.js",
    "es:test": "node scripts/test-elasticsearch.js",
    "es:test:performance": "node scripts/test-elasticsearch.js --performance",
    "es:sync": "node scripts/start-auto-sync.js --interval 30",
    "es:sync:full": "curl -X POST http://localhost:3000/api/search/sync -H 'Content-Type: application/json' -d '{\"type\":\"full\"}'",
    "es:sync:incremental": "curl -X POST http://localhost:3000/api/search/sync -H 'Content-Type: application/json' -d '{\"type\":\"incremental\"}'",
    "es:health": "curl http://localhost:9200/_cluster/health?pretty",
    "scraper:examples": "node scripts/run-scraper-examples.js",
    "scraper:test": "node scripts/run-scraper-examples.js test-connection",
    "scraper:quick": "node scripts/run-scraper-examples.js quick-product",
    "scraper:bulk": "node scripts/run-scraper-examples.js bulk-products",
    "scraper:orders": "node scripts/run-scraper-examples.js orders",
    "scraper:shop": "node scripts/run-scraper-examples.js shop-analysis",
    "scraper:cluster": "node scripts/run-scraper-examples.js cluster",
    "scraper:all": "node scripts/run-scraper-examples.js all",
    "proxy:demo": "node examples/advanced-proxy-usage.js",
    "proxy:test": "node examples/advanced-proxy-usage.js test",
    "proxy:dashboard": "node -e \"const {AdvancedProxySystem} = require('./src/services/proxy'); const sys = new AdvancedProxySystem(); sys.startDashboard().then(() => console.log('Dashboard started at http://localhost:3001'));\""
  },
  "keywords": [
    "shopee",
    "scraper",
    "nodejs",
    "puppeteer",
    "mongodb",
    "redis",
    "elasticsearch"
  ],
  "author": "barkahsp",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^9.0.2",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^4.15.0",
    "cheerio": "^1.0.0",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-prom-bundle": "^7.0.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.6.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongodb": "^6.16.0",
    "mongoose": "^8.15.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "prom-client": "^15.0.0",
    "puppeteer": "^24.9.0",
    "puppeteer-cluster": "^0.24.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-adblocker": "^2.13.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "rate-limiter-flexible": "^4.0.1",
    "redis": "^5.1.0",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "superagent": "^10.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "eslint": "^9.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "husky": "^8.0.3",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
